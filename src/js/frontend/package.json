{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.190.0",
    "@react-oauth/google": "latest",
    "axios": "^0.27.2",
    "formik": "^2.2.9",
    "history": "^5.3.0",
    "http-proxy-middleware": "^2.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-oauth": "^0.0.1",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0",
    "yup": "^0.32.11"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "integrationTest": "yarn playwright test",
    "unitTest": "react-scripts test tests/unit --maxWorkers=1",
    "eject": "react-scripts eject",
    "check-types": "tsc --project tsconfig.json --pretty --noEmit",
    "format": "yarn run prettier:fix && yarn run lint:fix",
    "lint": "npx eslint src test",
    "lint:fix": "yarn run lint --fix",
    "prettier": "npx prettier src test --check",
    "prettier:fix": "yarn run prettier --write"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@playwright/test": "^1.27.1",
    "@testing-library/dom": "^8.18.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "eslint": "latest",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "latest",
    "jest": "^29.2.2",
    "jest-fetch-mock": "^3.0.3",
    "prettier": "^2.7.1"
  },
  "engines": {
    "node": ">= 18.8.0",
    "npm": "please-use-yarn-add",
    "yarn": ">= 3.2.3"
  },
  "packageManager": "yarn@3.2.3",
  "description": "Grapl Frontend",
  "main": "index.tsx",
  "license": "MIT"
}
